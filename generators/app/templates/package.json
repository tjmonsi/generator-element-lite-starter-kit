    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "workbox-cli": "^3.6.3",
    "worker-loader": "^2.0.0"
  },
  "flat": true,
  "sideEffects": false,
  "size-limit": [],
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [
      "beforeAll",
      "afterAll",
      "jest"
    ]
  }
}
